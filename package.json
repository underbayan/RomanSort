{
  "name": "romansort",
  "displayName": "RomanSort",
  "description": "intelligent sort for C style language",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:roman.sort",
    "onCommand:roman.sort.reverse"
  ],
  "repository": "https://github.com/underbayan/roman-sort.git",
  "icon": "icon.png",
  "publisher": "RomanTai",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "roman.sort",
        "title": "roman.sort"
      },
      {
        "command": "roman.sort.reverse",
        "title": "roman.sort.reverse"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.57.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "charcodes": "^0.2.0",
    "lodash": "^4.17.21"
  }
